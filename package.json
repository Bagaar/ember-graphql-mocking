{
  "name": "@bagaar/ember-graphql-mocking",
  "version": "0.9.0",
  "description": "Ember addon for mocking GraphQL requests using Mock Service Worker (MSW).",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/Bagaar/ember-graphql-mocking",
  "license": "MIT",
  "author": "Bagaar",
  "typesVersions": {
    "*": {
      "test-support": [
        "declarations/addon-test-support/index.d.ts"
      ],
      "test-support/*": [
        "declarations/addon-test-support/*",
        "declarations/addon-test-support/*/index.d.ts"
      ],
      "*": [
        "declarations/addon/*",
        "declarations/addon/*/index.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "tsc --project tsconfig.declarations.json",
    "postpack": "rimraf declarations",
    "prepare": "husky install",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.23.3",
    "broccoli-funnel": "^3.0.5",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.7.0",
    "ember-cli-babel": "^8.2.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.7",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "@tsconfig/ember": "^3.0.3",
    "@types/lodash.merge": "^4.6.9",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.6.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.0.1",
    "graphql": "^16.8.1",
    "husky": "^9.0.10",
    "loader.js": "^4.7.0",
    "msw": "^2.0.9",
    "prettier": "^3.2.4",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "release-it": "^17.0.3",
    "rimraf": "^5.0.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-qunit": "^8.0.0",
    "ember-source": ">= 4.8.0",
    "graphql": "^16.0.0",
    "msw": "^2.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
